<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.course.web.mapper.ProgressMapper">
    <select id="findByUid" parameterType="Integer" resultType="com.company.course.web.entity.Progress">
        select * from `course`.`progress` where uid = #{$uid}
    </select>
</mapper>